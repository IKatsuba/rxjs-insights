{
  "manifest_version": 2,
  "version": "0.0.0",
  "name": "RxJS Insights",
  "description": "See through the observables.",
  "permissions": [
    "tabs",
    "activeTab",
    "webNavigation"
  ],
  "devtools_page": "devtools-page.html",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "dist/content-script.js"
      ],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": [
    "dist/page-script.js"
  ]
}
