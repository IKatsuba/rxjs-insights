{
  "name": "rxjs-insights",
  "version": "0.0.0",
  "scripts": {
    "lerna": "node lerna/node_modules/lerna/cli.js",
    "bootstrap": "npm install && npm --prefix lerna install && npm run lerna bootstrap",
    "build": "npm run lerna run build",
    "version-packages": "npm run lerna version --force-publish --exact --no-push",
    "publish-packages": "npm run lerna publish from-package",
    "version-packages-next": "npm run lerna version --force-publish --exact --no-push --canary",
    "publish-packages-next": "npm run lerna publish from-package --dist-tag=next"
  },
  "private": true,
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "esbuild": "^0.14.20",
    "esbuild-plugin-alias": "^0.2.1",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "glob": "^7.2.0",
    "jest": "^27.5.0",
    "patch-package": "^6.4.7",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5"
  }
}
