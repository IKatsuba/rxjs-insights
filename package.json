{
  "name": "rxjs-insights",
  "version": "0.0.0",
  "scripts": {
    "build": "npm run build --workspaces",
    "version-packages": "npm run version-package --workspaces --if-present",
    "publish-packages": "npm run publish-package --workspaces --if-present"
  },
  "private": true,
  "workspaces": {
    "packages": [
      "packages/instrumentation",
      "packages/recorder",
      "packages/tracer",
      "packages/locator",
      "packages/console",
      "packages/rxjs6",
      "packages/rxjs7",
      "packages/plugin-esbuild",
      "packages/plugin-webpack5",
      "packages/playground-esbuild",
      "packages/playground-webpack5"
    ]
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "esbuild": "^0.14.20",
    "esbuild-plugin-alias": "^0.2.1",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "glob": "^7.2.0",
    "jest": "^27.5.0",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5"
  }
}
